{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "administratorLogin": {
            "value": null
        },
        "administratorLoginPassword": {
            "value": null
        },
        "location": {
            "value": "East US"
        },
        "serverName": {
            "value": null
        },
        "skuCapacity": {
            "value": 2
        },
        "skuFamily": {
            "value": "Gen5"
        },
        "skuName": {
            "value": "GP_Gen5_2"
        },
        "skuSizeMB": {
            "value": 5120
        },
        "skuTier": {
            "value": "GeneralPurpose"
        },
        "version": {
            "value": "10"
        },
        "backupRetentionDays": {
            "value": 7
        },
        "geoRedundantBackup": {
            "value": "Disabled"
        },
        "firewallRules": {
            "value": {
                "name": "AllowRange",
                "startIpAddress": "128.0.0.0",
                "endIpAddress": "191.255.255.255"
            }
        },
        "virtualNetworkRules": {
            "value": {
                "name": "SampleRule",
                "virtualNetworksExternalid": null,
                "virtualNetworkSubnetId": null,
                "ignoreMissingVnetServiceEndpoint": false
            }
        },
        "serverConfigurations": {
            "value": {
                "configurations": [
                    {
                        "name" : "timezone",
                        "value" : "UTC",
                        "source" : "system-default"
                    }
                ]
            }
        },
        "databases": {
            "value": {
                "name": "sampledb1",
                "charset": "UTF8",
                "collation": "English_United States.1252"
            }
        }
    }
}